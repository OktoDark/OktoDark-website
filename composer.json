{
  "name": "oktodark/website",
  "description": "OktoDark Studios Main Website",
  "minimum-stability": "dev",
  "license": "proprietary",
  "authors": [
    {
      "name": "Razvan George H. (Viruzzz)",
      "email": "contact@oktodark.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "ext-intl": "^1.1",
    "doctrine/doctrine-bundle": "^1.8",
    "doctrine/orm": "^2.6",
    "easycorp/easyadmin-bundle": "^1.17",
    "erusev/parsedown": "^1.6",
    "ezyang/htmlpurifier": "^4.9",
    "patreon/patreon": "^0.3.1",
    "sensio/framework-extra-bundle": "^5.1",
    "sensiolabs/security-checker": "dev-master",
    "symfony/asset": "4.2.0-beta2",
    "symfony/browser-kit": "4.2.0-beta2",
    "symfony/console": "^4.0",
    "symfony/expression-language": "4.2.0-beta2",
    "symfony/flex": "^1.1",
    "symfony/form": "4.2.0-beta2",
    "symfony/framework-bundle": "4.2.0-beta2",
    "symfony/maker-bundle": "^1.1",
    "symfony/monolog-bundle": "^3.3",
    "symfony/orm-pack": "^1.0",
    "symfony/profiler-pack": "^1.0",
    "symfony/security-bundle": "4.2.0-beta2",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "4.2.0-beta2",
    "symfony/twig-bundle": "4.2.0-beta2",
    "symfony/validator": "4.2.0-beta2",
    "symfony/webpack-encore-bundle": "dev-master",
    "symfony/yaml": "4.2.0-beta2",
    "twig/extensions": "^1.5",
    "twig/twig": "^2.4",
    "white-october/pagerfanta-bundle": "^1.1"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "easycorp/easy-deploy-bundle": "^1.0",
    "friendsofphp/php-cs-fixer": "^2.12",
    "symfony/debug-bundle": "4.2.0-beta2",
    "symfony/dotenv": "4.2.0-beta2",
    "symfony/phpunit-bridge": "4.2.0-beta2",
    "symfony/web-profiler-bundle": "4.2.0-beta2",
    "symfony/web-server-bundle": "4.2.0-beta2"
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C240QDH68XFC7KR57RMF0202",
      "allow-contrib": true
    }
  }
}
