{
  "name": "oktodark/website",
  "type": "project",
  "description": "OktoDark Studios Main Website",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "proprietary",
  "authors": [
    {
      "name": "Razvan George H. (Viruzzz)",
      "email": "contact@oktodark.com"
    }
  ],
  "replace": {
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "require": {
    "php": ">=8.2",
    "ext-iconv": "*",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/doctrine-migrations-bundle": "^3.4",
    "doctrine/orm": "^2.20",
    "easycorp/easyadmin-bundle": "^4.23",
    "erusev/parsedown": "^1.7",
    "paragonie/sodium_compat": "dev-master",
    "patreon/patreon": "dev-master",
    "symfony/asset": "^7.2",
    "symfony/console": "^7.2",
    "symfony/discord-notifier": "^7.2",
    "symfony/dotenv": "^7.2",
    "symfony/expression-language": "^7.2",
    "symfony/flex": "^2.4",
    "symfony/form": "^7.2",
    "symfony/framework-bundle": "^7.2",
    "symfony/html-sanitizer": "^7.2",
    "symfony/intl": "^7.2",
    "symfony/mailer": "^7.2",
    "symfony/monolog-bundle": "^3.10",
    "symfony/notifier": "^7.2",
    "symfony/polyfill-intl-messageformatter": "^1.31",
    "symfony/proxy-manager-bridge": "6.4.*",
    "symfony/rate-limiter": "^7.2",
    "symfony/runtime": "^7.2",
    "symfony/security-bundle": "^7.2",
    "symfony/security-csrf": "^7.2",
    "symfony/string": "^7.2",
    "symfony/translation": "^7.2",
    "symfony/twig-bundle": "^7.2",
    "symfony/validator": "^7.2",
    "symfony/webpack-encore-bundle": "^2.2",
    "symfony/yaml": "^7.2",
    "symfonycasts/reset-password-bundle": "^1.23",
    "symfonycasts/verify-email-bundle": "^1.17",
    "twig/intl-extra": "^3.19",
    "twig/twig": "^3.19"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^8.2",
    "doctrine/doctrine-fixtures-bundle": "^3.7",
    "friendsofphp/php-cs-fixer": "^3.68",
    "symfony/browser-kit": "^7.2",
    "symfony/css-selector": "^7.2",
    "symfony/debug-bundle": "^7.2",
    "symfony/maker-bundle": "^1.62",
    "symfony/phpunit-bridge": "^7.2",
    "symfony/stopwatch": "^7.2",
    "symfony/web-profiler-bundle": "^7.2"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "platform": {
      "php": "8.2.0"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "^7.2"
    }
  }
}
