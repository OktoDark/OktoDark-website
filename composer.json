{
  "name": "oktodark/website",
  "type": "project",
  "description": "OktoDark Studios Main Website",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "license": "proprietary",
  "authors": [
    {
      "name": "Razvan George H. (Viruzzz)",
      "email": "contact@oktodark.com"
    }
  ],
  "require": {
    "php": ">=8.4",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^3.1",
    "doctrine/doctrine-migrations-bundle": "^3.7",
    "doctrine/orm": "^3.5",
    "easycorp/easyadmin-bundle": "^4.27",
    "erusev/parsedown": "^1.7",
    "paragonie/sodium_compat": "dev-master",
    "patreon/patreon": "dev-master",
    "symfony/asset": "^8.0",
    "symfony/console": "^8.0",
    "symfony/discord-notifier": "^8.0",
    "symfony/dotenv": "^8.0",
    "symfony/expression-language": "^8.0",
    "symfony/flex": "^2.10",
    "symfony/form": "^8.0",
    "symfony/framework-bundle": "^8.0",
    "symfony/html-sanitizer": "^8.0",
    "symfony/intl": "^8.0",
    "symfony/mailer": "^8.0",
    "symfony/messenger": "^8.0",
    "symfony/monolog-bundle": "^4.0",
    "symfony/notifier": "^8.0",
    "symfony/polyfill-intl-messageformatter": "^1.33",
    "symfony/rate-limiter": "^8.0",
    "symfony/runtime": "^8.0",
    "symfony/security-bundle": "^8.0",
    "symfony/security-csrf": "^8.0",
    "symfony/string": "^8.0",
    "symfony/translation": "^8.0",
    "symfony/twig-bundle": "^8.0",
    "symfony/validator": "^8.0",
    "symfony/webpack-encore-bundle": "^2.4",
    "symfony/yaml": "^8.0",
    "symfonycasts/reset-password-bundle": "^1.23",
    "symfonycasts/verify-email-bundle": "^1.17",
    "twig/intl-extra": "^3.22",
    "twig/twig": "^3.22"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^8.4",
    "doctrine/doctrine-fixtures-bundle": "^4.3",
    "friendsofphp/php-cs-fixer": "^3.93",
    "phpunit/phpunit": "^11.5",

    "symfony/browser-kit": "^8.0",
    "symfony/css-selector": "^8.0",
    "symfony/debug-bundle": "^8.0",
    "symfony/maker-bundle": "^1.65",
    "symfony/stopwatch": "^8.0",
    "symfony/web-profiler-bundle": "^8.0"
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*",
    "symfony/polyfill-php83": "*",
    "symfony/polyfill-php84": "*"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "platform": {
      "php": "8.4.0"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "^8.0"
    }
  }
}
