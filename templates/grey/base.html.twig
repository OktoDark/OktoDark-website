<!DOCTYPE html>
<html class="boxed" lang="{{ app.request.locale }}" dir="ltr">
    <head>
        {% block head %}
        <title>{% block title %}{% endblock %} - {% for s in settings %} {{ s.siteName }}</title>
        <meta name="format-detection" content="telephone=no">
        <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta charset="utf-8">
        <link rel="icon" href="{{s.siteCDN}}/themes/grey/images/favicon.ico" type="image/x-icon">
        <!-- Stylesheets-->
        <link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Roboto+Slab:100,300,400,700%7CMontserrat:400,700,800">
        <link rel="stylesheet" href="//cdn.materialdesignicons.com/2.4.85/css/materialdesignicons.min.css">
        <link rel="stylesheet" href="{{s.siteCDN}}/themes/grey/css/bootstrap.css">
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.1.1/css/all.css" integrity="sha384-O8whS3fhG2OnA5Kas0Y9l3cfpmYjapjI0E4theH4iuMD+pLhbf6JI0jIMfYcK3yZ" crossorigin="anonymous">
        <link rel="stylesheet" href="{{s.siteCDN}}/themes/grey/css/style.css" id="main-styles-link">
        {% endfor %}
        {% endblock %}
        <meta property="og:url"           content="https://www.oktodark.com" />
        <meta property="og:type"          content="oktodark" />
        <meta property="og:title"         content="OktoDark" />
        <meta property="og:description"   content="OktoDark Studios" />
        <meta property="og:image"         content="https://www.your-domain.com/path/image.jpg" />
        <script src='https://www.google.com/recaptcha/api.js'></script>
    </head>
    <body id="{% block body_id %}{% endblock %}">
    <!-- Load Facebook SDK for JavaScript -->
    <div id="fb-root"></div>
    <script>(function(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "https://connect.facebook.net/en_US/sdk.js#xfbml=1";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));</script>
    {% include("@theme/loading.html.twig") %}
    <div class="page">
        {% include("@theme/navbar.html.twig") %}
        {% block body %}
            {% block main %}{% endblock %}
        {% endblock %}

        <!-- Page Footer -->
        <footer class="section">
            <div class="footer-classic">
                <div class="container">
                    <div class="row row-50 justify-content-xl-between">
                        <div class="col-lg-12">
                            <div class="row row-50">
                                <div class="col">
                                    <h4 class="footer-classic-title">{{ 'information.text'|trans }}</h4>
                                    <ul class="list list-font-accent list-marked">
                                        <li><i class="fas fa-info-circle"></i> <a href="{{ path('faq') }}">{{ 'title.faq'|trans }}</a></li>
                                        <li><i class="fas fa-briefcase"></i> <a href="{{ path('services') }}">{{ 'title.services'|trans }}</a></li>
                                        <li><i class="fab fa-wpforms"></i> <a href="{{ path('contact') }}">{{ 'title.contact'|trans }}</a></li>
                                    </ul>
                                </div>
                                <div class="col">
                                    <h4 class="footer-classic-title">{{ 'footer.get_in_touch_text'|trans }}</h4>
                                    <ul class="list list-font-accent list-marked">
                                        <li><i class="fab fa-facebook"></i> <a href="https://www.facebook.com/OktoDark">Facebook</a></li>
                                        <li><i class="fab fa-twitter"></i> <a href="https://www.twitter.com/OktoDark">Twitter</a></li>
                                        <li><i class="fab fa-youtube"></i> <a href="https://www.youtube.com/channel/UCDqAin2dpl6R9XIPFQe4Vsg">Youtube</a></li>
                                        <li><i class="fab fa-google-plus"></i> <a href="https://plus.google.com/u/0/b/103588266793943361093/103588266793943361093">Google+</a></li>
                                    </ul>
                                </div>
                                <div class="col">
                                    <h4 class="footer-classic-title">{{ 'footer.source_code_text'|trans }}</h4>
                                    <ul class="list list-font-accent list-marked">
                                        <li><i class="fab fa-github"></i> <a href="https://www.github.com/OktoDark">Github</a></li>
                                        <li><i class="fab fa-bitbucket"></i> <a href="https://www.bitbucket.com/OktoDark">Bitbucket</a></li>
                                    </ul>
                                </div>
                                <div class="col">
                                    <h4 class="footer-classic-title">{{ 'footer.live_stream_text'|trans }}</h4>
                                    <ul class="list list-font-accent list-marked">
                                        <li><i class="fab fa-twitch"></i> <a href="https://www.twitch.com/OktoDark">Twitch</a></li>
                                    </ul>
                                </div>
                                <div class="col">
                                    <h4 class="footer-classic-title">{{ 'footer.discourse_text'|trans }}</h4>
                                    <ul class="list list-font-accent list-marked">
                                        <li><i class="fab fa-discord"></i> <a href="https://discord.gg/zAA8Gdz">Discord</a></li>
                                    </ul>
                                </div>
                                <div class="col">
                                    <h4 class="footer-classic-title">{{ 'footer.support_us_text'|trans }}</h4>
                                    <ul class="list list-font-accent list-marked">
                                        <li><i class="fab fa-patreon"></i> <a href="https://www.patreon.com/OktoDark">Patreon</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="sub-footer-section">
                <div class="container">
                    <div class="footer-language_selector">
                        <i class="fas fa-globe"></i> {{ 'language.selector'|trans }}:
                        {% for locale in locales() %}
                            <a href="{{ path(app.request.get('_route', 'home'), app.request.get('_route_params', [])|merge({_locale: locale.code})) }}">{{ locale.name|capitalize }}</a>
                        {% endfor %}
                    </div>
                    <!-- Rights-->
                    <p class="rights">
                        <span>&copy;</span>
                        <span class="copyright-year"></span>
                        <span>{% for s in settings %}{{s.siteName}}. {{ 'footer.all_rights_reserved_text'|trans }}{% endfor %}</span>
                        <span><a href="{{ path('privacy-policy') }}">{{ 'privacy_policy.text'|trans }}</a></span>
                    </p>
                </div>
            </div>
        </footer>
    </div>
    <div class="snackbars" id="form-output-global"></div>
    <!-- Javascript-->
    {% for s in settings %}
        <script src="{{s.siteCDN}}/themes/grey/js/core.min.js"></script>
        <script src="{{s.siteCDN}}/themes/grey/js/script.js"></script>
    {% endfor %}
    <!-- End Footer -->
    </body>
</html>
